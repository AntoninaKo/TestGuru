<tr align="center">
  <td><%= test.id %></td>
  <td><%= test.name %></td>
  <td><%= level_test(test) %></td>
  <td><%= test.questions.count %></td>
  <td><%= link_to t('actions.show'), admin_test_path(test) %></td>
  <td><%= link_to t('actions.edit'), edit_admin_test_path(test) %></td>
  <td>
    <%= link_to t('actions.delete'), admin_test_path(test),
                method: :delete,
                data: { confirm: t('actions.delete_confirmation') } %>
  </td>
  <td><%= button_to 'Start', start_test_path(test) %></td>

</tr>
