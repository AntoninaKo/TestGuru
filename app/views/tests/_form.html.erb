<%= form_with model: @test, local: true do |form| %>

  <p>
    <%= form.label :name %>
    <%= form.text_field :name %>
  </p><br>

  <p>
    <%= form.label :level %>
    <%= form.text_field :level %>
  </p><br>

  <p>
    <%= form.label :category_id %>
    <%= form.collection_select :category_id, Category.all, :id, :title, prompt: true  %>
  </p><br>

  <p>
    <%= form.label :author_id %>
    <%= form.collection_select :author_id, User.all, :id, :name, prompt: true %>
  </p><br>

  <p><%= form.submit %></p>
  <%#= render 'common/errors', resource: @test %>
<% end %>
